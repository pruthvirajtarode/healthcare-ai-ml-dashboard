{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/diagrams/(.*)",
      "destination": "/diagrams/$1"
    },
    {
      "source": "/backend/(.*)",
      "destination": "/backend/$1"
    },
    {
      "source": "/scripts/(.*)",
      "destination": "/scripts/$1"
    },
    {
      "source": "/(index.html)?",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}